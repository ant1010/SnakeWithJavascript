{
    "version": 2,
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static"
        },
        {
            "src": "./index.js",
            "use": "@vercel/node-server"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "public/index.html"
        }
    ]
}
